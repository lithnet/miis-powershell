    <CmdletObject verb="Disconnect" noun="CSObject">
      <Name>Disconnect-CSObject</Name>
      <GeneralHelp>
        <Synopsis>Disconnects a connector space object from the metaverse</Synopsis>
        <Description>The Disconnect-CSObject cmdlet allows you to disconnect a specified connector space object from the connector space. If disconnecting the connector space object will result in the metaverse object being deleted, the cmdlet will prompt for confirmation before proceeding.</Description>
        <Notes />
        <InputType />
        <InputUrl />
        <InputTypeDescription />
        <ReturnType />
        <ReturnUrl />
        <ReturnTypeDescription />
      </GeneralHelp>
      <ParamSets>
        <CommandParameterSetInfo2 Name="__AllParameterSets" Params="CSObject Explicit Force Verbose Debug ErrorAction WarningAction InformationAction ErrorVariable WarningVariable InformationVariable OutVariable OutBuffer PipelineVariable" />
      </ParamSets>
      <Syntax>
        <string>Disconnect-CSObject [-CSObject] &lt;CSObject&gt; [[-Explicit] &lt;SwitchParameter&gt;] [[-Force] &lt;SwitchParameter&gt;] [-InformationAction &lt;ActionPreference&gt;] [-InformationVariable &lt;String&gt;]</string>
      </Syntax>
      <Parameters>
        <ParameterDescription type="CSObject" varLen="false" required="true" dynamic="false" pipeRemaining="false" pipe="true" pipeProp="false" isPos="true" pos="1" globbing="false">
          <Name>CSObject</Name>
          <Attributes>
            <string>System.Management.Automation.ParameterAttribute</string>
            <string>System.Management.Automation.ValidateNotNullOrEmptyAttribute</string>
          </Attributes>
          <Aliases />
          <Description>The connector space object to disconnect</Description>
          <DefaultValue />
        </ParameterDescription>
        <ParameterDescription type="SwitchParameter" varLen="false" required="false" dynamic="false" pipeRemaining="false" pipe="false" pipeProp="false" isPos="true" pos="2" globbing="false">
          <Name>Explicit</Name>
          <Attributes>
            <string>System.Management.Automation.ParameterAttribute</string>
          </Attributes>
          <Aliases />
          <Description>Specifies if the connector should be made an explicit disconnector. By default, a normal disconnector is made</Description>
          <DefaultValue>false</DefaultValue>
        </ParameterDescription>
        <ParameterDescription type="SwitchParameter" varLen="false" required="false" dynamic="false" pipeRemaining="false" pipe="false" pipeProp="false" isPos="true" pos="3" globbing="false">
          <Name>Force</Name>
          <Attributes>
            <string>System.Management.Automation.ParameterAttribute</string>
          </Attributes>
          <Aliases />
          <Description />
          <DefaultValue />
        </ParameterDescription>
        <ParameterDescription type="ActionPreference" varLen="false" required="false" dynamic="false" pipeRemaining="false" pipe="false" pipeProp="false" isPos="false" pos="named" globbing="false">
          <Name>InformationAction</Name>
          <Attributes>
            <string>System.Management.Automation.ParameterAttribute</string>
            <string>System.Management.Automation.AliasAttribute</string>
          </Attributes>
          <Aliases>
            <string>infa</string>
          </Aliases>
          <Description />
          <DefaultValue />
        </ParameterDescription>
        <ParameterDescription type="String" varLen="false" required="false" dynamic="false" pipeRemaining="false" pipe="false" pipeProp="false" isPos="false" pos="named" globbing="false">
          <Name>InformationVariable</Name>
          <Attributes>
            <string>System.Management.Automation.ParameterAttribute</string>
            <string>System.Management.Automation.AliasAttribute</string>
            <string>System.Management.Automation.Internal.CommonParameters+ValidateVariableName</string>
          </Attributes>
          <Aliases>
            <string>iv</string>
          </Aliases>
          <Description />
          <DefaultValue />
        </ParameterDescription>
      </Parameters>
      <Examples>
        <Example>
          <Name>Disconnect a connector space object using the pipeline</Name>
          <Cmd>Get-CSObject -RDN "CN=John Smith" -MA "ADMA" | Disconnect-CSObject</Cmd>
          <Description />
          <Output />
        </Example>
        <Example>
          <Name>Disconnect a connector space object</Name>
          <Cmd>$cs = Get-CSObject -RDN "CN=John Smith" -MA "ADMA"
Disconnect-CSObject -CSObject $cs</Cmd>
          <Description />
          <Output />
        </Example>
        <Example>
          <Name>Explicitly disconnect a connector space object</Name>
          <Cmd>$cs = Get-CSObject -RDN "CN=John Smith" -MA "ADMA"
Disconnect-CSObject -CSObject $cs -Explicit</Cmd>
          <Description />
          <Output />
        </Example>
      </Examples>
