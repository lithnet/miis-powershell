  <Cmdlets>
    <CmdletObject verb="Clear" noun="RunHistory">
      <Name>Clear-RunHistory</Name>
      <GeneralHelp>
        <Synopsis>Clears the run history from the synchronization service</Synopsis>
        <Description>The Clear-RunHistory cmdlet allows you to clear the Synchronization Service's run history. You can clear all runs, all runs older than a specified date, or all runs older than a specified number of days. The cmdlet also provides the ability to save the runs that are cleared to a file</Description>
        <Notes>Saved run history files can be loaded using the Get-SavedRunHistory cmdlet</Notes>
        <InputType />
        <InputUrl />
        <InputTypeDescription />
        <ReturnType />
        <ReturnUrl />
        <ReturnTypeDescription />
      </GeneralHelp>
      <ParamSets>
        <CommandParameterSetInfo2 Name="ByDate" Params="BeforeDate File Verbose Debug ErrorAction WarningAction InformationAction ErrorVariable WarningVariable InformationVariable OutVariable OutBuffer PipelineVariable" />
        <CommandParameterSetInfo2 Name="ByDays" Params="DaysToKeep File Verbose Debug ErrorAction WarningAction InformationAction ErrorVariable WarningVariable InformationVariable OutVariable OutBuffer PipelineVariable" />
      </ParamSets>
      <Syntax>
        <string>Clear-RunHistory [[-BeforeDate] &lt;Nullable`1[DateTime]&gt;] [-File &lt;String&gt;] [-InformationAction &lt;ActionPreference&gt;] [-InformationVariable &lt;String&gt;]</string>
        <string>Clear-RunHistory [[-DaysToKeep] &lt;Nullable`1[Int32]&gt;] [-File &lt;String&gt;] [-InformationAction &lt;ActionPreference&gt;] [-InformationVariable &lt;String&gt;]</string>
      </Syntax>
      <Parameters>
        <ParameterDescription type="Nullable`1[DateTime]" varLen="true" required="false" dynamic="false" pipeRemaining="false" pipe="false" pipeProp="false" isPos="true" pos="1" globbing="false">
          <Name>FromDate</Name>
          <Attributes>
            <string>System.Management.Automation.ParameterAttribute</string>
          </Attributes>
          <Aliases />
          <Description />
          <DefaultValue />
        </ParameterDescription>
        <ParameterDescription type="Nullable`1[Int32]" varLen="true" required="false" dynamic="false" pipeRemaining="false" pipe="false" pipeProp="false" isPos="true" pos="1" globbing="false">
          <Name>DaysToKeep</Name>
          <Attributes>
            <string>System.Management.Automation.ParameterAttribute</string>
          </Attributes>
          <Aliases />
          <Description>Specifies the number of days of run history to keep</Description>
          <DefaultValue />
        </ParameterDescription>
        <ParameterDescription type="Nullable`1[DateTime]" varLen="true" required="false" dynamic="false" pipeRemaining="false" pipe="false" pipeProp="false" isPos="true" pos="1" globbing="false">
          <Name>BeforeDate</Name>
          <Attributes>
            <string>System.Management.Automation.ParameterAttribute</string>
          </Attributes>
          <Aliases />
          <Description>Specifies the date of the latest run that will be deleted from the server. Runs after this specified date and time are preserved</Description>
          <DefaultValue />
        </ParameterDescription>
        <ParameterDescription type="String" varLen="false" required="false" dynamic="false" pipeRemaining="false" pipe="false" pipeProp="false" isPos="false" pos="named" globbing="false">
          <Name>File</Name>
          <Attributes>
            <string>System.Management.Automation.ParameterAttribute</string>
          </Attributes>
          <Aliases />
          <Description>The name of the file to save the runs to before they are cleared</Description>
          <DefaultValue />
        </ParameterDescription>
        <ParameterDescription type="ActionPreference" varLen="false" required="false" dynamic="false" pipeRemaining="false" pipe="false" pipeProp="false" isPos="false" pos="named" globbing="false">
          <Name>InformationAction</Name>
          <Attributes>
            <string>System.Management.Automation.ParameterAttribute</string>
            <string>System.Management.Automation.AliasAttribute</string>
          </Attributes>
          <Aliases>
            <string>infa</string>
          </Aliases>
          <Description />
          <DefaultValue />
        </ParameterDescription>
        <ParameterDescription type="String" varLen="false" required="false" dynamic="false" pipeRemaining="false" pipe="false" pipeProp="false" isPos="false" pos="named" globbing="false">
          <Name>InformationVariable</Name>
          <Attributes>
            <string>System.Management.Automation.ParameterAttribute</string>
            <string>System.Management.Automation.AliasAttribute</string>
            <string>System.Management.Automation.Internal.CommonParameters+ValidateVariableName</string>
          </Attributes>
          <Aliases>
            <string>iv</string>
          </Aliases>
          <Description />
          <DefaultValue />
        </ParameterDescription>
      </Parameters>
      <Examples>
        <Example>
          <Name>Clear the entire run history</Name>
          <Cmd>Clear-RunHistory</Cmd>
          <Description>Using the Clear-RunHistory cmdlet with no parameters will clear the entire run history from the server</Description>
          <Output />
        </Example>
        <Example>
          <Name>Clear runs older than specified number of days</Name>
          <Cmd>Clear-RunHistory -DaysToKeep 2</Cmd>
          <Description>Clears all items in the run history older than 2 days</Description>
          <Output />
        </Example>
        <Example>
          <Name>Clears run history older than the specified date</Name>
          <Cmd>Clear-RunHistory "2016-01-01"</Cmd>
          <Description>Clears all runs older than January 1, 2016</Description>
          <Output />
        </Example>
        <Example>
          <Name>Clear the run history, saving the runs to a file</Name>
          <Cmd>Clear-RunHistory -DaysToKeep 5 -File "C:\Logs\RunHistory.xml"</Cmd>
          <Description />
          <Output />
        </Example>
      </Examples>
